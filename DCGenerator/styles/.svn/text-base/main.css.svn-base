body {
	margin: 0px;
	font-family: verdana;
	font-size: 10px;	
}

div.maincontent {
	margin-left: 5px;
	margin-top: 5px;
}

div.title {
	font-decoration: outline;
	margin-left: 5px;
	margin-top: 5px;
	font-size: 36px;
	color: #CC0000;
	font-weight: bold;
}

div.breadcrumb {
	margin-left: 5px;
	font-size: 12px;
	color: #CC0000;
	margin-bottom: 5px;
	font-weight: bold;
}

span.datalabel {
	margin-left: 5px;
}

div.appitem {
	margin-bottom: 3px;	
}

div.activeappitem {
	margin-bottom: 3px;
	font-weight: bold;	
}

div.logo {
	margin-top: 5px;
	margin-left: 5px;
	margin-bottom: 5px;	
}

table.workflownav {
	margin-bottom: 5px;
	margin-left: 8px;
	margin-top: 5px;	
}

div.subhead {
	margin-left: 3px;
	margin-top: 3px;
	color: #CC0000;
	font-size: 12px;
	font-weight: bold;
}

div.sectiontitle {
	background-color: #CC0000;
	margin-left: 3px;
	margin-top: 3px;
	margin-bottom: 3px;
	color: #FFE500;
	font-size: 10px;
	font-weight: bold;
}

td.sectiontitle {
	background-color: #CC0000;		
}

table.workflownav {
	margin-bottom: 5px;
}

td.activeworkflowtab {
	border: rgb(0,0,0);
	color: #CC0000;
	padding: 10px;
	background-color: #FFE500;
	font-weight: bold;
}
.inactivelink A:visited {
	text-decoration: none;
	color: #FFE500;
}

.inactivelink A:link {
	text-decoration: none;
	color: #FFE500;
}
td.inactiveworkflowtab {
	border-right-style: solid;
	border-width: 1px;
	border-color: #FFE500;
	color: #FFE500;
	padding: 10px;
	background-color: #CC0000;
	font-weight: bold;
}

table.section {
	border: solid 1px #CC0000;	
	margin-left: 3px;
	margin-bottom: 20px;
	padding-bottom: 5px;
}

td.tableheader {
	padding: 5px;
	color: #CC0000;
	font-weight: bold;	
}

span.icon {
	margin-right: 5px;	
}

td.addstepline {
	padding: 5px;
	margin-bottom: 10px;	
}

td.leftnav {
	background-color: #FFE500;	
}
div.leftnav {
	font-size: 11p;
	color: #CC0000;
	margin-left: 5px;
	margin-top: 8px;	
}

select {
	font-family: verdana;
	font-size: 10px;
}

input {
	font-family: verdana;
	font-size: 10px;
}

div.separator {
	background-color: #000000;	
}

.submitbutton {
	margin-top: 5px;
	margin-bottom: 5px;
	border: 1px solid #000000;
	background: #CC0000;
	color: #FFE500;
}

span.envradio {
	padding-top: 3px;
}